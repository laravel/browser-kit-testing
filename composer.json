{
    "name": "laravel/browser-kit-testing",
    "description": "Provides backwards compatibility for BrowserKit testing in the latest Laravel release.",
    "keywords": ["laravel", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "laravel/framework": "^10.44|^11.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.4|^11.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/css-selector": "^6.2|^7.0",
        "symfony/dom-crawler": "^6.2|^7.0",
        "symfony/http-foundation": "^6.2|^7.0",
        "symfony/http-kernel": "^6.2|^7.0"
    },
    "require-dev": {
        "orchestra/testbench-core": "^8.21|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BrowserKitTesting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\BrowserKitTesting\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
